# Visit 3.1.4 log file
ScriptVersion = "3.1.4"
if ScriptVersion != Version():
    print "This script is for VisIt %s. It may not work with version %s" % (ScriptVersion, Version())
ShowAllWindows()
OpenDatabase("localhost:/usr/WS1/dorr/ESL/neumann_bc_2/COGENT/test_exec/MBSolver_slab/gradient_normal2d.hdf5", 0)
metadata = GetMetaData("localhost:/usr/WS1/dorr/ESL/neumann_bc_2/COGENT/test_exec/MBSolver_slab/gradient_normal2d.hdf5", -1)
AddPlot("Mesh", "Mesh", 1, 1)
HideActivePlots()
AddPlot("Pseudocolor", "component_0", 1, 1)
DrawPlots()
SetActivePlots(0)
AddOperator("Displace", 1)
DisplaceAtts = DisplaceAttributes()
DisplaceAtts.factor = 1
DisplaceAtts.variable = "_mapping_displacement"
SetOperatorOptions(DisplaceAtts, 0, 1)
SetActivePlots(1)
HideActivePlots()
AddOperator("Displace", 1)
DisplaceAtts = DisplaceAttributes()
DisplaceAtts.factor = 1
DisplaceAtts.variable = "_mapping_displacement"
SetOperatorOptions(DisplaceAtts, 0, 1)
HideActivePlots()
DrawPlots()
SetActivePlots(1)
