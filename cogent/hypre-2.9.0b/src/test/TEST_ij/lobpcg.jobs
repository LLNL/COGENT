#=============================================================================
# ij: LOBPCG cases
#=============================================================================

mpirun -np 2 ./ij -lobpcg -vrand 2 -seed 1 -tol 1.e-4 -vout 1 > lobpcg.out.vout.1

# use vectors computed by previous run as the initial
mpirun -np 2 ./ij -lobpcg -vfromfile -seed 1 > lobpcg.out.vfromfile
